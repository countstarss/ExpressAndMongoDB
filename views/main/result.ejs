
<div class="author">
    <h1 class="author_heading">Search Result</h1>
</div>

<section class="articles">
    <ul class="article_ul">
        <% data.forEach(post => { %>
            <li>
                <a href="/post/<%= post._id %>">
                    <%= post.title %>
                    <span class="article-list_date"><%= post.createdAt.toDateString() %></span>
                </a> 
            </li>
        <% }) %>
    </ul>
</section>
